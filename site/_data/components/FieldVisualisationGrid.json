{
  "name": "Field Visualisation (Grid)",
  "nickname": "Field Grid",
  "icon": "icon_field_grid_vis.png",
  "description": "Translate a field into a grid based visualisation. Outputs a number constrained by a range which can be used to then make a color or shape.",
  "category": "Groundhog",
  "subcategory": "Mapping",
  "inputs": [
    {
      "name": "Field",
      "id": "F",
      "description": "Field generated by a Field Component",
      "optional": false,
      "type": "SurfaceParameter"
    },
    {
      "name": "Bounds",
      "id": "B",
      "description": "Curve boundary to constraint the visualisation to a certain area",
      "optional": true,
      "type": "CurveParameter"
    },
    {
      "name": "Domain Start",
      "id": "DS",
      "description": "Starting value representing the 'bottom' value",
      "optional": true,
      "type": "NumberParameter"
    },
    {
      "name": "Domain End",
      "id": "DE",
      "description": "Ending value representing the 'top' value",
      "optional": true,
      "type": "NumberParameter"
    }
  ],
  "outputs": [
    {
      "name": "Points",
      "id": "P",
      "description": "Grid Points",
      "optional": false,
      "type": "PointParameter"
    },
    {
      "name": "Parameters",
      "id": "P",
      "description": "Grid Parameters",
      "optional": false,
      "type": "NumberParameter(\"Parameters\", \"P\", \"Grid Parameter"
    }
  ]
}